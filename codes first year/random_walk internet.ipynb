    "import random\n",
    "def randomwalk_2(n):\n",
    "    x,y=0,0\n",
    "    for i in range(n+1):\n",
    "        (dx,dy)=random.choice([(0,-1),(-1,0),(1,0),(0,1)])\n",
    "        x+=dx\n",
    "        y+=dy\n",
    "    return(x,y)   \n",
    "for i in range(100):\n",
    " print( randomwalk_2(5),end=' ')"
